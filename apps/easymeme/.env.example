# EasyMeme local config (minimal-first)
# Copy to `.env` and change only what you need.

# --- Basic runtime ---
POSTGRES_PASSWORD=easyweb3
EASYMEME_USER_ID=default
CORS_ALLOWED_ORIGINS=http://localhost:3000

# --- Chain/data providers (optional; defaults are usable for local run) ---
BSC_RPC_HTTP=https://bsc-dataseed.bnbchain.org
BSC_RPC_WS=
BSCSCAN_API_KEY=

# --- Service auth (optional but recommended) ---
SERVICE_TOKEN_EASYMEME=dev-token
EASYMEME_API_KEY=
EASYMEME_API_HMAC_SECRET=

# --- Wallet (required only when using managed wallet trading) ---
WALLET_MASTER_KEY=

# --- OpenClaw gateway/notify (optional) ---
OPENCLAW_GATEWAY_PORT=18789
OPENCLAW_GATEWAY_TOKEN=
OPENCLAW_GATEWAY_BIND=lan
EASYMEME_NOTIFY_CHANNEL=
EASYMEME_NOTIFY_TO=
